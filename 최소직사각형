#include <string>
#include <vector>

using namespace std;

int solution(vector<vector<int>> sizes) {
    int answer = 0;
    
    for(int i = 0; i<sizes.size(); i++){
        
        int temp = 0;
        
        if(sizes[i][0] < sizes[i][1]){
            
            temp = sizes[i][1];
            sizes[i][1] = sizes[i][0];
            sizes[i][0] = temp;
            
            
        }
        
    }
    
    int max1 = sizes[0][0];
    int max2 = sizes[0][1];
    
    for(int i = 0; i<sizes.size(); i++){
        
        if(max1 < sizes[i][0]){
            
            max1 = sizes[i][0];
            
        }
        if(max2 < sizes[i][1]){
            
            max2 = sizes[i][1];
            
        }
    }
    
    answer = max1 * max2;
    
    return answer;
}
