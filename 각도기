#include <iostream>

int main()
{

    int angle;
    int res = 0;

    std::cin >> angle;

    if(angle > 0 && angle < 90) {

        res = 1;

    }
    else if(angle == 90) {

        res = 2;

    }
    else if (angle > 90 && angle < 180) {

        res = 3;

    }
    else if (angle == 180) {

        res = 4;

    }

    std::cout << res;

    return 0;
    
}
